<?jelly escape-by-default='true' ?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="${%Name}" field="name" help="/help/parameter/name.html">
        <f:textbox/>
    </f:entry>
    <f:repeatable field="tabs" add="${%Add Tab}" header="${%Tab}">
        <f:entry field="name" title="${%Tab Name}">
            <f:textbox/>
        </f:entry>
        <f:nested>
            <f:repeatableHeteroProperty field="parameters" hasHeader="true" addCaption="${%Add Parameter}"/>
        </f:nested>
    </f:repeatable>
</j:jelly>
